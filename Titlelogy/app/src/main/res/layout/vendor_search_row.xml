<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:id="@+id/card_reult"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="15dp"
    android:layout_marginRight="15dp"
    android:layout_marginTop="8dp"
    card_view:cardCornerRadius="2dp"
    card_view:contentPadding="5dp"
    card_view:cardBackgroundColor="#ffffff"
    card_view:cardElevation="5dp"
    card_view:cardMaxElevation="8dp"
    android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar">


    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:paddingTop="10dp"
            android:weightSum="3">


            <TextView
                android:text="NSS-FL"
                android:textStyle="bold"
                android:gravity="start"
                android:textColor="#205DA9"
                android:textSize="15sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/vendor_result_subclient"
                android:layout_weight="1"/>

            <TextView
                android:text="-"
                android:textStyle="bold"
                android:gravity="start"
                android:textColor="#000000"
                android:textSize="15sp"
                android:layout_marginLeft="5dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/vendor_search_hiphane"
                android:layout_weight="1"/>

            <TextView
                android:text="123456789"
                android:textColor="#205DA9"
                android:textSize="15sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/vendor_result_orderno"
                android:layout_marginLeft="5dp"
                android:layout_weight="1"/>

        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:weightSum="2">


            <TextView
                android:text="STATUS"
                android:textStyle="bold"
                android:gravity="start"
                android:textColor="#205DA9"
                android:textSize="15sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/vendor_result1_status"
                android:layout_weight="1"/>


            <TextView
                android:text="-"
                android:textStyle="bold"
                android:gravity="start"
                android:textColor="#000000"
                android:textSize="15sp"
                android:layout_marginLeft="5dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/vendor_search_hiphane2"
                android:layout_weight="1"/>
            <TextView
                android:text="Upload Cancelled"
                android:textColor="#205DA9"
                android:textSize="15sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:id="@+id/vendor_result_status"
                android:layout_weight="1"/>

        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            >

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_producttype"
                android:tint="@color/icongrey"
                android:layout_marginLeft="10dp" />

            <TextView
                android:text="Current Owner Seaarch"
                android:textColor="@color/fontcolor"
                android:textSize="15sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/vendor_result_producttype"
                android:layout_marginLeft="30dp"
                />

        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_address"
                android:tint="@color/icongrey"
                android:layout_marginLeft="10dp"/>

            <TextView
                android:text="Michael C. Irish"
                android:textColor="@color/fontcolor"
                android:textSize="15sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/vendor_result_borrowername"
                android:layout_marginLeft="30dp" />

        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_state"
                android:tint="@color/icongrey"
                android:layout_marginLeft="10dp"/>


            <TextView
                android:text="Oklahama"
                android:textColor="@color/fontcolor"
                android:textSize="15sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/vendor_result_state"
                android:layout_marginLeft="30dp" />


            <TextView
                android:text=","
                android:textColor="@color/fontcolor"
                android:textSize="15sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/vendor_result_comma1" />


            <TextView
                android:text="Mcinthosh"
                android:textColor="@color/fontcolor"
                android:textSize="15sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/vendor_result_county"
                android:layout_marginLeft="2dp" />



        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:paddingBottom="10dp">



            <TextView
                android:text="16 Rcgdggg"
                android:textSize="15sp"
                android:textColor="@color/fontcolor"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/vendor_result_address"
                android:layout_marginLeft="53dp"/>

        </LinearLayout>



    </LinearLayout>
</android.support.v7.widget.CardView>